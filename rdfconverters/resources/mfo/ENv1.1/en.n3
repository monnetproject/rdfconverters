<http://www.dfki.de/lt/time.owl#SynchronicProperty> a owl:Class;
    rdfs:subClassOf rdf:Property;
    owl:disjointWith <http://www.dfki.de/lt/time.owl#DiachronicProperty> .

en:ShareholderValue a owl:Class;
    rdfs:comment """shareholders of a company are Actors, and the number of shares is given in %;
in case a name is specified, it should be attached to the actor's property "name""""^^xsd:string .

en:State a owl:Class;
    rdfs:subClassOf en:Actor;
    owl:disjointWith en:Company,
        en:Group,
        en:Person .

en:Group a owl:Class;
    rdfs:subClassOf en:Actor;
    owl:disjointWith en:Company,
        en:Person,
        en:State .

en:Person a owl:Class;
    rdfs:subClassOf en:Actor;
    owl:disjointWith en:Company,
        en:Group,
        en:State .

en:Actor a owl:Class;
    rdfs:comment "the superclass of Person, Company, State, and Group"^^xsd:string .

xsd:monetary a rdfs:Datatype .

en:Company a owl:Class;
    rdfs:comment """the Euronext pages come with a specified ICB subsector value;
we have decided to make these ICB classes subclasses of en:Company, but this needs to be specified separately and is NOT part of THIS ontology;
we would recommend to have an "interfacing" ontology that links an ICB ontology to EN, although an extension of THIS ontology which add about 200 subclasses to en:Company might also be fine;
at the moment, only
  activity
  city
  country
  source
are regarded as MULTI-lingual properties (use language tag!)"""^^xsd:string;
    rdfs:subClassOf en:Actor;
    owl:disjointWith en:Group,
        en:Person,
        en:State .

<http://www.dfki.de/lt/time.owl#DiachronicProperty> a owl:Class;
    rdfs:subClassOf rdf:Property;
    owl:disjointWith <http://www.dfki.de/lt/time.owl#SynchronicProperty> .

