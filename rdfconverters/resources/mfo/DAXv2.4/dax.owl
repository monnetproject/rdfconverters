<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:time="http://www.dfki.de/lt/time.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:dax="http://www.dfki.de/lt/dax.owl#"
  xml:base="http://www.dfki.de/lt/dax.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >(C) Hans-Ulrich Krieger, DFKI-LT</rdfs:comment>
  </owl:Ontology>
  <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#monetary"/>
  <owl:Class rdf:ID="AutoPartsAndEquipment">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Automobile"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="de">Autoteile und -zubehör</rdfs:label>
    <rdfs:label xml:lang="en">Auto Parts &amp; Equipment</rdfs:label>
    <rdfs:definition xml:lang="en">Producers of parts and accessories for motor vehicles and motorcycles (including tyres and batteries)</rdfs:definition>
    <rdfs:definition xml:lang="de">Hersteller von Teilen und Zubehör für Automobile und Motorräder, inkl. Reifen und Batterien</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="FixedLineTelecommunication">
    <rdfs:label xml:lang="en">Fixed-Line Telecommunication</rdfs:label>
    <rdfs:label xml:lang="de">Festnetz-Telekommunikation</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Telecommunication"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="en">Telecommunications carriers mainly providing fixed-line local and long-distance services</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen, die vorwiegend im Festnetzbereich Orts- und Ferngespräche anbieten</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="DiversifiedFinancial">
    <rdfs:definition xml:lang="de">Finanzdienstleister, die keine Universalbank sind, aber eine Palette von Finanzdienstleistungen vertreiben</rdfs:definition>
    <rdfs:definition xml:lang="en">Financial services providers that do not have universal bank status, but who distribute a range of financial services</rdfs:definition>
    <rdfs:label xml:lang="de">Allgemeine Finanzdienstleister</rdfs:label>
    <rdfs:label xml:lang="en">Diversified Financial</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FinancialServices"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ConstructionAndEngineering">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Construction"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="de">Unternehmen, die in der Entwicklung und dem Bau von Gebäuden und Infrastruktur (Straßenbau) tätig sind (außer Fertighäuser)</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies active in the development and construction of buildings and infrastructure projects (e.g. roads) (excluding prefabricated homes)</rdfs:definition>
    <rdfs:label xml:lang="de">Bau- und Ingenieurswesen</rdfs:label>
    <rdfs:label xml:lang="en">Construction &amp; Engineering</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="ReInsurance">
    <rdfs:label xml:lang="de">Rückversicherungen</rdfs:label>
    <rdfs:label xml:lang="en">Re-Insurance</rdfs:label>
    <rdfs:definition xml:lang="de">Unternehmen, die hauptsachlich Rückversicherungen abschließen</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies mainly underwriting reinsurance cover</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Insurance"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="FirstQuotationBoard">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TransparencyStandard"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="IndustrialMachinery">
    <rdfs:label xml:lang="de">Industriemaschinenbau</rdfs:label>
    <rdfs:label xml:lang="en">Industrial Machinery</rdfs:label>
    <rdfs:definition xml:lang="de">Hersteller von Industriemaschinen und - komponenten. Hierzu gehören u. a. Hersteller von Werkzeugmaschinen, Kompressoren, Druckmaschinen u. ä.</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of industrial machinery or related components, such as machine tools, compressors, printing machines, etc.</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Industrial"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Construction">
    <rdfs:label xml:lang="de">Bauindustrie</rdfs:label>
    <rdfs:label xml:lang="en">Construction</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Company"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RetailCatalog">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Retail"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="en">Retailers selling mainly by mail order, which are not categorized under "Retail, Internet"</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen des Einzelhandels, die ihre Güter vorwiegend über Versand anbieten, und nicht zur Branche "Internethandel" gehören</rdfs:definition>
    <rdfs:label xml:lang="en">Retail, Catalog</rdfs:label>
    <rdfs:label xml:lang="de">Versandhandel</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="AutomobileManufacturers">
    <rdfs:label xml:lang="de">Automobilhersteller</rdfs:label>
    <rdfs:label xml:lang="en">Automobile Manufacturers</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Automobile"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="de">Unternehmen, die hauptsächlich Personenfahrzeuge, Kleinlastwagen und Motorräder herstellen</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies primarily active in the production of passenger vehicles, small lorries and motorcycles</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="RegulatedMarket">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MarketSegment"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="OpenMarket"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">regulated market</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="BasicResources">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Company"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="de">Energie &amp; Rohstoffe</rdfs:label>
    <rdfs:label xml:lang="en">Basic Resources</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="RetailMultiline">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Retail"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Retail, Multiline</rdfs:label>
    <rdfs:label xml:lang="de">Kaufhäuser</rdfs:label>
    <rdfs:definition xml:lang="en">Retail companies with a broad product range (department stores)</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen des Einzelhandels mit einem breitgefächerten Produktangebot (Kaufhäuser)</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="HeavyMachinery">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Industrial"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="de">Hersteller von Großmaschinen, die nicht nur für den Einsatz an einem festen Ort bestimmt sind. Hierzu gehören Lastkraftwagen, Schiffe, landwirtschaftliche Maschinen u. ä.</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of large but mobile machinery, such as heavy goods vehicles, ships, agricultural machines, etc.</rdfs:definition>
    <rdfs:label xml:lang="de">Großmaschinenbau</rdfs:label>
    <rdfs:label xml:lang="en">Heavy Machinery</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#MarketSegment">
    <rdfs:label xml:lang="en">market segment</rdfs:label>
    <rdfs:label xml:lang="de">Börsensegment</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Software">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Company"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Software</rdfs:label>
    <rdfs:label xml:lang="de">Software</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="MortgageBanks">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Banks"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="de">Hypothekenbanken</rdfs:label>
    <rdfs:label xml:lang="en">Mortgage Banks</rdfs:label>
    <rdfs:definition xml:lang="de">Spezialbanken, die ausschließlich oder vorwiegend langfristige Kredite gegen Grundpfandrechte oder Kommunaldarlehen gewähren</rdfs:definition>
    <rdfs:definition xml:lang="en">Specialist banks exclusively (or predominantly) extending longterm loans against liens on real property, or communal loan</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="PlainSoftware">
    <rdfs:label xml:lang="en">Software</rdfs:label>
    <rdfs:label xml:lang="de">Software</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Software"/>
    <rdfs:definition xml:lang="en">Companies focused on the development of standard or specialized software solutions</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen, die hauptsächlich Standard- oder Spezialsoftware entwickeln</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="HouseholdAppliancesAndHouseware">
    <rdfs:definition xml:lang="de">Hersteller von Haushalts- und Gartenartikeln (Besteck, Geschirr, Reinigungsmittel) und Haushaltsgeräten (Küchenmaschinen)</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of household and garden products (such as cutlery, dishes or cleaning products), or of household appliances ("white goods")</rdfs:definition>
    <rdfs:label xml:lang="de">Haushaltsgeräte und - waren</rdfs:label>
    <rdfs:label xml:lang="en">Household Appliances &amp; Houseware</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Consumer"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PrivateEquityAndVentureCapital">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#FinancialServices"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="de">Beteiligungsunternehmen, die in diversifizierte Geschäftsfelder investieren</rdfs:definition>
    <rdfs:definition xml:lang="en">Holding companies investing in diversified business areas</rdfs:definition>
    <rdfs:label xml:lang="de">Private Equity &amp; Venture Capital</rdfs:label>
    <rdfs:label xml:lang="en">Private Equity &amp; Venture Capital</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="TransportationServices">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TransportationAndLogistics"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="en">Providers of infrastructural or other specialized transport services (including airport operators, road or rail networks, tour operators, etc.)</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen, die Infrastruktur- und Spezialdienstleistungen im Transportbereich anbieten. Hierzu gehören Flughafen-, Strassen- und Schienennetzbetreiber, Tourismus- Unternehmen u. a.</rdfs:definition>
    <rdfs:label xml:lang="en">Transportation Services</rdfs:label>
    <rdfs:label xml:lang="de">Transport-Dienstleistungen</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="SecuritiesBrokers">
    <rdfs:definition xml:lang="de">Unternehmen, die im Wertpapiergeschäft tätig sind wie Broker, Online-Banken, Börsen u. a.</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies active in the securities business, such as brokers, online banks, exchanges, etc.</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#FinancialServices"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="de">Wertpapierhandel</rdfs:label>
    <rdfs:label xml:lang="en">Securities Brokers</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Person">
    <rdfs:label xml:lang="de">Person</rdfs:label>
    <rdfs:label xml:lang="en">person</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#OpenMarket">
    <rdfs:subClassOf rdf:resource="#MarketSegment"/>
    <rdfs:label xml:lang="en">open market</rdfs:label>
    <rdfs:label xml:lang="de">Freiverkehr</rdfs:label>
    <owl:disjointWith rdf:resource="#RegulatedMarket"/>
  </owl:Class>
  <owl:Class rdf:ID="Advertising">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Media"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="en">Companies offering advertising, marketing and/or public relations services</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen, die Werbe-, Marketing- und/oder PR-Dienste anbieten</rdfs:definition>
    <rdfs:label xml:lang="en">Advertising</rdfs:label>
    <rdfs:label xml:lang="de">Werbung</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Electricity">
    <rdfs:label xml:lang="en">Electricity</rdfs:label>
    <rdfs:label xml:lang="de">Elektrizitätsversorgung</rdfs:label>
    <rdfs:definition xml:lang="en">Companies generating and/or distributing electricity (including operators 
    of power stations)</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen, die Strom erzeugen und/oder vertreiben. Hierzu gehören auch Betreiber von Kernkraftwerken</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Utilities"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LeisureGoodsAndServices">
    <rdfs:definition xml:lang="de">Hersteller von Freizeitartikeln (Sportausstattung, Spielzeug, Fahrräder) sowie Restaurant-, Hotel- und Casinobetreiber</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of leisure goods (such as sports equipment, toys, bicycles), as well as restaurant, hotel or casino operators</rdfs:definition>
    <rdfs:label xml:lang="de">Freizeitgüter &amp; Dienstleistungen</rdfs:label>
    <rdfs:label xml:lang="en">Leisure Goods &amp; Services</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Consumer"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Media">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Company"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Media</rdfs:label>
    <rdfs:label xml:lang="de">Medien</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Biotechnology">
    <rdfs:label xml:lang="en">Biotechnology</rdfs:label>
    <rdfs:label xml:lang="de">Biotechnologie</rdfs:label>
    <rdfs:definition xml:lang="en">Companies mainly active in developing, producing, marketing or licensing products based on biotechnological research</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen, die hauptsächlich in der Entwicklung, Herstellung, Vermarktung oder Lizenzierung von Produkten tätig sind, die auf biotechnologischer Forschung beruhen</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="PharmaAndHealthcare"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Healthcare">
    <rdfs:label xml:lang="en">Healthcare</rdfs:label>
    <rdfs:label xml:lang="de">Gesundheitswesen</rdfs:label>
    <rdfs:definition xml:lang="en">Owners/operators of healthcare institutions (such as hospitals or nursing homes), providers of healthcare services (e.g. dialysis) and providers of medical material (such as syringes, swabs, etc.)</rdfs:definition>
    <rdfs:definition xml:lang="de">Besitzer und Betreiber von Einrichtungen des Gesundheitswesens wie Krankenhäuser und Pflegeheime, sowie Dienstleister im Bereich Gesundheit (z. B. Dialyse) und Anbieter von medizinischen Hilfsprodukten (z. B. Spritzen und Tupfer)</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#PharmaAndHealthcare"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="IndustrialProductsAndServices">
    <rdfs:label xml:lang="de">Industrieprodukte &amp; Dienstleistungen</rdfs:label>
    <rdfs:label xml:lang="en">Industrial Products &amp; Services</rdfs:label>
    <rdfs:definition xml:lang="de">Hersteller von sonstigen Produkten und Dienstleistungen zum Einsatz in der Industrie (z. B. Marktforschung, Personaldienstleistungen, industrieller Großhandel oder Entsorgung)</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers/providers of other industrial products or services (e.g. market research, human resources, industrial wholesalers, waste disposal)</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Industrial"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RenewableEnergies">
    <rdfs:label xml:lang="de">Erneuerbare Energien</rdfs:label>
    <rdfs:label xml:lang="en">Renewable Energies</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Industrial"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="de">Unternehmen, die im Bereich der Entwicklung von Anlagen für alternative und/oder erneuerbare Energien (Solartechnik, Windkraftanlagen etc.) tätig sind</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies developing equipment for alternative and/or renewable energy generation, such as solar technology or wind-powered turbines</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="ContainersAndPackaging">
    <rdfs:label xml:lang="de">Container &amp; Verpackungen</rdfs:label>
    <rdfs:label xml:lang="en">Containers &amp; Packaging</rdfs:label>
    <rdfs:definition xml:lang="de">Unternehmen, die sich auf Verpackungen aller Art spezialisiert haben</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies specializing in all kinds of packaging</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Industrial"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="OilAndGas">
    <rdfs:subClassOf rdf:resource="#BasicResources"/>
    <rdfs:label xml:lang="de">Öl &amp; Erdgas</rdfs:label>
    <rdfs:label xml:lang="en">Oil &amp; Gas</rdfs:label>
    <rdfs:definition xml:lang="de">Unternehmen, die Öl und Gas fördern und nicht der Industriegruppe "Öl und Gas, Distribution" angehören</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies drilling for oil and gas, which do not fall under the "Oil &amp; Gas Distribution" category</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:about="http://www.dfki.de/lt/time.owl#SynchronicProperty">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </owl:Class>
  <owl:Class rdf:ID="Pharmaceuticals">
    <rdfs:label xml:lang="en">Pharmaceuticals</rdfs:label>
    <rdfs:label xml:lang="de">Pharmazeutika</rdfs:label>
    <rdfs:definition xml:lang="en">Companies researching, developing or producing pharmaceuticals (including veterinary products)</rdfs:definition>
    <rdfs:definition xml:lang="de">Unternehmen, die in der Erforschung, Entwicklung oder Herstellung von Pharmazeutika tätig sind. Hierzu gehören auch veterinärmedizinische Präparate</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#PharmaAndHealthcare"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="AdvancedIndustrialEquipment">
    <rdfs:label xml:lang="de">High-Tech Industrieanlagen</rdfs:label>
    <rdfs:label xml:lang="en">Advanced Industrial Equipment</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Industrial"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="de">Unternehmen, die hoch entwickelte Technologien zur Erstellung industrieller High-Tech-Produkte einsetzen oder im Anlagenbau für solche Produkte tätig sind. Beispiele hierfür sind Hersteller von Lasern, Robotern, optischen Speichermedien</rdfs:definition>
    <rdfs:definition xml:lang="en">Companies using sophisticated technology or providing engineering for the production of high-tech industrial goods (for example, producers of lasers, robots or optical storage media)</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="Food">
    <rdfs:label xml:lang="de">Nahrungsmittel, Tabak</rdfs:label>
    <rdfs:label xml:lang="en">Food</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FoodAndBeverages"/>
    </rdfs:subClassOf>
    <rdfs:definition xml:lang="de">Hersteller von Nahrungsmitteln und Tabakwaren jeder Art</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of food and tobacco products of all kinds</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="BuildingMaterials">
    <rdfs:label xml:lang="en">Building Materials</rdfs:label>
    <rdfs:label xml:lang="de">Baumaterialien</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Construction"/>
    <rdfs:definition xml:lang="de">Hersteller von Grundbaukomponenten für die Bauindustrie wie Zement, Bodenbeläge, Türen, Fenster u. ä.</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of basic construction materials, such as cement, flooring, doors, windows, etc.</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:ID="PersonalProducts">
    <rdfs:label xml:lang="de">Kosmetika</rdfs:label>
    <rdfs:label xml:lang="en">Personal Products</rdfs:label>
    <rdfs:definition xml:lang="de">Hersteller von Kosmetik- und Körperpflegeprodukten</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of cosmetics and personal care products</rdfs:definition>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Consumer"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#FinancialServices">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Company"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="de">Finanzdienstleistungen</rdfs:label>
    <rdfs:label xml:lang="en">Financial Services</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="ForestAndPaperProducts">
    <rdfs:subClassOf rdf:resource="#BasicResources"/>
    <rdfs:label xml:lang="en">Forest &amp; Paper Products</rdfs:label>
    <rdfs:label xml:lang="de">Forst- und Papierprodukte</rdfs:label>
    <rdfs:definition xml:lang="de">Hersteller von Holz und Holzprodukten in Rohform, sowie Produzenten von Papier</rdfs:definition>
    <rdfs:definition xml:lang="en">Producers of wood or basic wood products, and paper producers</rdfs:definition>
  </owl:Class>
  <owl:Class rdf:about="#Retail">
    <rdfs:label xml:lang="en">Retail</rdfs:label>
    <rdfs:label xml:lang="de">Handel</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Company"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ChemicalsCommodity">
    <rdfs:label xml:lang="de">Chemieprodukte, Zwischenerzeugnisse</rdfs:label>
