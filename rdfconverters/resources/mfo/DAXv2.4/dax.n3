@prefix dax: <http://www.dfki.de/lt/dax.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.dfki.de/lt/dax.owl> a owl:Ontology;
    rdfs:comment "(C) Hans-Ulrich Krieger, DFKI-LT"^^xsd:string .

dax:AdvancedIndustrialEquipment a owl:Class;
    rdfs:label "Advanced Industrial Equipment"@en,
        "High-Tech Industrieanlagen"@de;
    rdfs:definition "Companies using sophisticated technology or providing engineering for the production of high-tech industrial goods (for example, producers of lasers, robots or optical storage media)"@en,
        "Unternehmen, die hoch entwickelte Technologien zur Erstellung industrieller High-Tech-Produkte einsetzen oder im Anlagenbau für solche Produkte tätig sind. Beispiele hierfür sind Hersteller von Lasern, Robotern, optischen Speichermedien"@de;
    rdfs:subClassOf dax:Industrial .

dax:Advertising a owl:Class;
    rdfs:label "Advertising"@en,
        "Werbung"@de;
    rdfs:definition "Companies offering advertising, marketing and/or public relations services"@en,
        "Unternehmen, die Werbe-, Marketing- und/oder PR-Dienste anbieten"@de;
    rdfs:subClassOf dax:Media .

dax:Airlines a owl:Class;
    rdfs:label "Airlines"@en,
        "Fluggesellschaften"@de;
    rdfs:definition "Aviation companies mainly carrying passengers"@en,
        "Unternehmen, die vorwiegend im Passagierflugverkehr tätig sind"@de;
    rdfs:subClassOf dax:TransportationAndLogistics .

dax:AutoPartsAndEquipment a owl:Class;
    rdfs:label "Auto Parts & Equipment"@en,
        "Autoteile und -zubehör"@de;
    rdfs:definition "Hersteller von Teilen und Zubehör für Automobile und Motorräder, inkl. Reifen und Batterien"@de,
        "Producers of parts and accessories for motor vehicles and motorcycles (including tyres and batteries)"@en;
    rdfs:subClassOf dax:Automobile .

dax:AutomobileManufacturers a owl:Class;
    rdfs:label "Automobile Manufacturers"@en,
        "Automobilhersteller"@de;
    rdfs:definition "Companies primarily active in the production of passenger vehicles, small lorries and motorcycles"@en,
        "Unternehmen, die hauptsächlich Personenfahrzeuge, Kleinlastwagen und Motorräder herstellen"@de;
    rdfs:subClassOf dax:Automobile .

dax:Beverages a owl:Class;
    rdfs:label "Beverages"@en,
        "Getränke"@de;
    rdfs:definition "Producers of beverages of all kinds"@en,
        "Unternehmen, die Getränke aller Art herstellen"@de;
    rdfs:subClassOf dax:FoodAndBeverages .

dax:Biotechnology a owl:Class;
    rdfs:label "Biotechnologie"@de,
        "Biotechnology"@en;
    rdfs:comment ""^^xsd:string;
    rdfs:definition "Companies mainly active in developing, producing, marketing or licensing products based on biotechnological research"@en,
        "Unternehmen, die hauptsächlich in der Entwicklung, Herstellung, Vermarktung oder Lizenzierung von Produkten tätig sind, die auf biotechnologischer Forschung beruhen"@de;
    rdfs:subClassOf dax:PharmaAndHealthcare .

dax:Broadcasting a owl:Class;
    rdfs:label "Broadcasting"@en,
        "Rundfunk & Fernsehen"@de;
    rdfs:definition "Anbieter von Kabel und Satellitenübertragungsdiensten, sowie von Radio- und Fernsehprogrammen"@de,
        "Providers of cable and satellite transmission services, and radio/television broadcasters"@en;
    rdfs:subClassOf dax:Media .

dax:BuildingMaterials a owl:Class;
    rdfs:label "Baumaterialien"@de,
        "Building Materials"@en;
    rdfs:definition "Hersteller von Grundbaukomponenten für die Bauindustrie wie Zement, Bodenbeläge, Türen, Fenster u. ä."@de,
        "Producers of basic construction materials, such as cement, flooring, doors, windows, etc."@en;
    rdfs:subClassOf dax:Construction .

dax:ChemicalsCommodity a owl:Class;
    rdfs:label "Chemicals, Commodity"@en,
        "Chemieprodukte, Zwischenerzeugnisse"@de;
    rdfs:definition "Producers of simple, standardized chemical products"@en,
        "Unternehmen, die vorwiegend einfache, normierte chemische Produkte herstellen"@de;
    rdfs:subClassOf dax:Chemicals .

dax:ChemicalsSpeciality a owl:Class;
    rdfs:label "Chemicals, Speciality"@en,
        "Spezial-Chemieprodukte"@de;
    rdfs:definition "Producers of non-standard, specialty chemical products"@en,
        "Unternehmen, die vorwiegend nicht normierte, chemische Spezialprodukte herstellen"@de;
    rdfs:subClassOf dax:Chemicals .

dax:ClothingAndFootwear a owl:Class;
    rdfs:label "Clothing & Footwear"@en,
        "Kleidung & Schuhe"@de;
    rdfs:definition "Companies producing mainly textile goods and shoes, including related cleaning services"@en,
        "Unternehmen, die hauptsächlich Textilien und Schuhe herstellen oder Pflegedienstleistungen für diese Produkte anbieten"@de;
    rdfs:subClassOf dax:Consumer .

dax:CommunicationsTechnology a owl:Class;
    rdfs:label "Communications Technology"@en,
        "Kommunikationstechnologie"@de;
    rdfs:definition "Companies developing telecommunications technology and/or products (such as user devices or network components)"@en,
        "Unternehmen, die im Bereich der Telekommunikation Technologien und/oder Produkte entwickeln (z. B. Endgeräte und Netzwerkkomponenten)"@de;
    rdfs:subClassOf dax:Technology .

dax:ConstructionAndEngineering a owl:Class;
    rdfs:label "Bau- und Ingenieurswesen"@de,
        "Construction & Engineering"@en;
    rdfs:definition "Companies active in the development and construction of buildings and infrastructure projects (e.g. roads) (excluding prefabricated homes)"@en,
        "Unternehmen, die in der Entwicklung und dem Bau von Gebäuden und Infrastruktur (Straßenbau) tätig sind (außer Fertighäuser)"@de;
    rdfs:subClassOf dax:Construction .

dax:ConsumerElectronics a owl:Class;
    rdfs:label "Consumer Electronics"@en,
        "Unterhaltungselektronik"@de;
    rdfs:definition "Companies producing mainly consumer electronics goods (such as TV sets, VCRs/camcorders HiFi equipment, etc.) (This excludes producers focusing on computers and telecommunications equipment.)"@en,
        "Unternehmen, die hauptsächlich Unterhaltungselektronik (TV, Video- und Camcorder, HIFI etc.) herstellen. Ausgeschlossen sind Firmen, die  vorwiegend Computer- und Telekommunikationsprodukte erzeugen"@de;
    rdfs:subClassOf dax:Consumer .

dax:ContainersAndPackaging a owl:Class;
    rdfs:label "Container & Verpackungen"@de,
        "Containers & Packaging"@en;
    rdfs:definition "Companies specializing in all kinds of packaging"@en,
        "Unternehmen, die sich auf Verpackungen aller Art spezialisiert haben"@de;
    rdfs:subClassOf dax:Industrial .

dax:CreditBanks a owl:Class;
    rdfs:label "Credit Banks"@en,
        "Kreditbanken"@de;
    rdfs:definition "Commercial and universal banks which do not fall into the categories of \"Mortgage Banks\" or \"Financial Services Providers\""@en,
        "Kredit- und Universalbanken, die nicht aufgrund ihres Schwerpunkts unter Hypothekenbanken oder Finanzdienstleistern eingeordnet sind"@de;
    rdfs:subClassOf dax:Banks .

dax:DiversifiedFinancial a owl:Class;
    rdfs:label "Allgemeine Finanzdienstleister"@de,
        "Diversified Financial"@en;
    rdfs:definition "Financial services providers that do not have universal bank status, but who distribute a range of financial services"@en,
        "Finanzdienstleister, die keine Universalbank sind, aber eine Palette von Finanzdienstleistungen vertreiben"@de;
    rdfs:subClassOf dax:FinancialServices .

dax:Electricity a owl:Class;
    rdfs:label "Electricity"@en,
        "Elektrizitätsversorgung"@de;
    rdfs:definition """Companies generating and/or distributing electricity (including operators 
    of power stations)"""@en,
        "Unternehmen, die Strom erzeugen und/oder vertreiben. Hierzu gehören auch Betreiber von Kernkraftwerken"@de;
    rdfs:subClassOf dax:Utilities .

dax:ElectronicComponentsAndHardware a owl:Class;
    rdfs:label "Electronic Components & Hardware"@en,
        "Elektronische Komponenten & Hardware"@de;
    rdfs:definition "Producers of electronic components (printed circuit boards, integrated circuits, smart cards) or computer hardware (PCs, monitors, etc.)"@en,
        "Unternehmen, die elektronische Bauelemente (Leiterplatten, integrierte Schaltkreise, Smart Cards) oder Computerhardware PCs, Monitore etc.) produzieren"@de;
    rdfs:subClassOf dax:Technology .

dax:FixedLineTelecommunication a owl:Class;
    rdfs:label "Festnetz-Telekommunikation"@de,
        "Fixed-Line Telecommunication"@en;
    rdfs:definition "Telecommunications carriers mainly providing fixed-line local and long-distance services"@en,
        "Unternehmen, die vorwiegend im Festnetzbereich Orts- und Ferngespräche anbieten"@de;
    rdfs:subClassOf dax:Telecommunication .

dax:Food a owl:Class;
    rdfs:label "Food"@en,
        "Nahrungsmittel, Tabak"@de;
    rdfs:definition "Hersteller von Nahrungsmitteln und Tabakwaren jeder Art"@de,
        "Producers of food and tobacco products of all kinds"@en;
    rdfs:subClassOf dax:FoodAndBeverages .

dax:ForestAndPaperProducts a owl:Class;
    rdfs:label "Forest & Paper Products"@en,
        "Forst- und Papierprodukte"@de;
    rdfs:definition "Hersteller von Holz und Holzprodukten in Rohform, sowie Produzenten von Papier"@de,
        "Producers of wood or basic wood products, and paper producers"@en;
    rdfs:subClassOf dax:BasicResources .

dax:Healthcare a owl:Class;
    rdfs:label "Gesundheitswesen"@de,
        "Healthcare"@en;
    rdfs:definition "Besitzer und Betreiber von Einrichtungen des Gesundheitswesens wie Krankenhäuser und Pflegeheime, sowie Dienstleister im Bereich Gesundheit (z. B. Dialyse) und Anbieter von medizinischen Hilfsprodukten (z. B. Spritzen und Tupfer)"@de,
        "Owners/operators of healthcare institutions (such as hospitals or nursing homes), providers of healthcare services (e.g. dialysis) and providers of medical material (such as syringes, swabs, etc.)"@en;
    rdfs:subClassOf dax:PharmaAndHealthcare .

dax:HeavyMachinery a owl:Class;
    rdfs:label "Großmaschinenbau"@de,
        "Heavy Machinery"@en;
    rdfs:definition "Hersteller von Großmaschinen, die nicht nur für den Einsatz an einem festen Ort bestimmt sind. Hierzu gehören Lastkraftwagen, Schiffe, landwirtschaftliche Maschinen u. ä."@de,
        "Producers of large but mobile machinery, such as heavy goods vehicles, ships, agricultural machines, etc."@en;
    rdfs:subClassOf dax:Industrial .

dax:HomeConstructionAndFurnishings a owl:Class;
    rdfs:label "Eigenheimbau & Möbel"@de,
        "Home Construction & Furnishings"@en;
    rdfs:definition "Hersteller von Fertighäusern, Heimwerkerbedarf und Einrichtungsgegenständen wie Möbel und Lampen"@de,
        "Producers of prefabricated homes, DIY products and furnishings (such as furniture or lighting)"@en;
    rdfs:subClassOf dax:Consumer .

dax:HouseholdAppliancesAndHouseware a owl:Class;
    rdfs:label "Haushaltsgeräte und - waren"@de,
        "Household Appliances & Houseware"@en;
    rdfs:definition "Hersteller von Haushalts- und Gartenartikeln (Besteck, Geschirr, Reinigungsmittel) und Haushaltsgeräten (Küchenmaschinen)"@de,
        "Producers of household and garden products (such as cutlery, dishes or cleaning products), or of household appliances (\"white goods\")"@en;
    rdfs:subClassOf dax:Consumer .

dax:ITServices a owl:Class;
    rdfs:label "IT-Dienstleistungen"@de,
        "IT-Services"@en;
    rdfs:definition "Companies active in IT consulting, IT operations, systems integration, etc."@en,
        "Unternehmen, die in den Bereichen IT- Beratung, IT-Betrieb, Systemintegration u. ä. tätig sind"@de;
    rdfs:subClassOf dax:Software .

dax:IndustrialDiversified a owl:Class;
    rdfs:label "Allgemeine Industrieunternehmen"@de,
        "Industrial, Diversified"@en;
    rdfs:definition "Companies with activities across various industrial sectors (including holding companies investing in different sectors)"@en,
        "Unternehmen, die in mehreren verschiedenen industriellen Bereichen tätig sind. Hierzu gehören auch Beteiligungsunternehmen, die in unterschiedlichen Branchen investieren"@de;
    rdfs:subClassOf dax:Industrial .

dax:IndustrialGases a owl:Class;
    rdfs:label "Industrial Gases"@en,
        "Industriegase"@de;
    rdfs:definition "Hersteller von Industriegasen"@de,
        "Producers of industrial gases"@en;
    rdfs:subClassOf dax:Chemicals .

dax:IndustrialMachinery a owl:Class;
    rdfs:label "Industrial Machinery"@en,
        "Industriemaschinenbau"@de;
    rdfs:definition "Hersteller von Industriemaschinen und - komponenten. Hierzu gehören u. a. Hersteller von Werkzeugmaschinen, Kompressoren, Druckmaschinen u. ä."@de,
        "Producers of industrial machinery or related components, such as machine tools, compressors, printing machines, etc."@en;
    rdfs:subClassOf dax:Industrial .

dax:IndustrialProductsAndServices a owl:Class;
    rdfs:label "Industrial Products & Services"@en,
        "Industrieprodukte & Dienstleistungen"@de;
    rdfs:definition "Hersteller von sonstigen Produkten und Dienstleistungen zum Einsatz in der Industrie (z. B. Marktforschung, Personaldienstleistungen, industrieller Großhandel oder Entsorgung)"@de,
        "Producers/providers of other industrial products or services (e.g. market research, human resources, industrial wholesalers, waste disposal)"@en;
    rdfs:subClassOf dax:Industrial .

dax:Internet a owl:Class;
    rdfs:label "Internet"@de,
        "Internet"@en;
    rdfs:definition "Companies providing and developing Internet infrastructure (Internet access, portals, software, etc.)"@en,
        "Unternehmen, die Internet-Infrastruktur (Internet-Zugang, -Portale, -Software etc.) bereitstellen und entwickeln"@de;
    rdfs:subClassOf dax:Software .

dax:LeisureGoodsAndServices a owl:Class;
    rdfs:label "Freizeitgüter & Dienstleistungen"@de,
        "Leisure Goods & Services"@en;
    rdfs:definition "Hersteller von Freizeitartikeln (Sportausstattung, Spielzeug, Fahrräder) sowie Restaurant-, Hotel- und Casinobetreiber"@de,
        "Producers of leisure goods (such as sports equipment, toys, bicycles), as well as restaurant, hotel or casino operators"@en;
    rdfs:subClassOf dax:Consumer .

dax:Logistics a owl:Class;
    rdfs:label "Logistics"@en,
        "Logistik"@de;
    rdfs:definition "Providers of industrial transport services (land transport/aviation cargo/freight shipping)"@en,
        "Unternehmen, die im industriellen Transport via Land, Luft und Wasser tätig sind"@de;
    rdfs:subClassOf dax:TransportationAndLogistics .

dax:MedicalTechnology a owl:Class;
    rdfs:label "Medical Technology"@en,
        "Medizintechnik"@de;
    rdfs:definition "Hersteller von technologischen Produkten und Apparaten für den Gesundheitsbereich, wie Herzschrittmacher, Dialysegeräte, UV-Therapie-Systeme"@de,
        "Producers of technological products and devices used in healthcare, such as pacemakers, dialysis equipment or UV therapy systems"@en;
    rdfs:subClassOf dax:PharmaAndHealthcare .

dax:Mining a owl:Class;
    rdfs:label "Bergbau"@de,
        "Mining"@en;
    rdfs:definition "Companies extracting commodities such as iron ore, minerals, aluminium, coal, diamonds or similar substances"@en,
        "Unternehmen, die Rohstoffe (Eisenerz, Mineralstoffe, Aluminium, Kohle, Diamanten etc.) aus der Erde abbauen"@de;
    rdfs:subClassOf dax:BasicResources .

dax:MortgageBanks a owl:Class;
    rdfs:label "Hypothekenbanken"@de,
        "Mortgage Banks"@en;
    rdfs:definition "Specialist banks exclusively (or predominantly) extending longterm loans against liens on real property, or communal loan"@en,
        "Spezialbanken, die ausschließlich oder vorwiegend langfristige Kredite gegen Grundpfandrechte oder Kommunaldarlehen gewähren"@de;
    rdfs:subClassOf dax:Banks .

dax:MoviesAndEntertainment a owl:Class;
    rdfs:label "Film & Unterhaltung"@de,
        "Movies & Entertainment"@en;
    rdfs:definition "Companies producing (or trading in) entertainment products and services, including producers, distributors and broadcasters of feature films and television shows, music producers and distributors, theatre operators and sports teams"@en,
        "Unternehmen, die in der Herstellung von oder dem Handel mit Unterhaltungsprodukten und -diensten tätig sind, einschließlich Unternehmen, die Spielfilme und Fernsehshows produzieren, vertreiben und ausstrahlen, Musik-Produzenten und -vertreiber, Theater und Sportmannschaften"@de;
    rdfs:subClassOf dax:Media .

dax:MultiUtilites a owl:Class;
    rdfs:label "Allgemeine Energieversorgung"@de,
        "Multi-Utilites"@en;
    rdfs:definition "Companies active across various energy sectors"@en,
        "Unternehmen, die in verschiedenen Branchen der Energieversorgung tätig sind"@de;
    rdfs:subClassOf dax:Utilities .

dax:OilAndGas a owl:Class;
    rdfs:label "Oil & Gas"@en,
        "Öl & Erdgas"@de;
    rdfs:definition "Companies drilling for oil and gas, which do not fall under the \"Oil & Gas Distribution\" category"@en,
        "Unternehmen, die Öl und Gas fördern und nicht der Industriegruppe \"Öl und Gas, Distribution\" angehören"@de;
    rdfs:subClassOf dax:BasicResources .

dax:OilAndGasDistribution a owl:Class;
    rdfs:label "Oil & Gas Distribution"@en,
        "Öl- und Gasversorgung"@de;
    rdfs:definition "Unternehmen, die vorwiegend Energie in Form von Öl und Gas vertreiben"@de,
        "Utilities mainly providing energy in the form of oil and gas"@en;
    rdfs:subClassOf dax:Utilities .

dax:PersonalProducts a owl:Class;
    rdfs:label "Kosmetika"@de,
        "Personal Products"@en;
    rdfs:definition "Hersteller von Kosmetik- und Körperpflegeprodukten"@de,
        "Producers of cosmetics and personal care products"@en;
    rdfs:subClassOf dax:Consumer .

dax:Pharmaceuticals a owl:Class;
    rdfs:label "Pharmaceuticals"@en,
        "Pharmazeutika"@de;
    rdfs:definition "Companies researching, developing or producing pharmaceuticals (including veterinary products)"@en,
        "Unternehmen, die in der Erforschung, Entwicklung oder Herstellung von Pharmazeutika tätig sind. Hierzu gehören auch veterinärmedizinische Präparate"@de;
    rdfs:subClassOf dax:PharmaAndHealthcare .

dax:PlainInsurance a owl:Class;
    rdfs:label "Insurance"@en,
        "Versicherungen"@de;
    rdfs:definition "Companies mainly underwriting policies for life, accident, health or home contents cover"@en,
        "Unternehmen, die hauptsächlich Lebens-, Unfall-, Kranken-, Hausratversicherungen etc. abschließen"@de;
    rdfs:subClassOf dax:Insurance .

dax:PlainSoftware a owl:Class;
    rdfs:label "Software"@de,
        "Software"@en;
    rdfs:definition "Companies focused on the development of standard or specialized software solutions"@en,
        "Unternehmen, die hauptsächlich Standard- oder Spezialsoftware entwickeln"@de;
    rdfs:subClassOf dax:Software .

dax:PrivateEquityAndVentureCapital a owl:Class;
    rdfs:label "Private Equity & Venture Capital"@de,
        "Private Equity & Venture Capital"@en;
    rdfs:definition "Beteiligungsunternehmen, die in diversifizierte Geschäftsfelder investieren"@de,
        "Holding companies investing in diversified business areas"@en;
    rdfs:subClassOf dax:FinancialServices .

dax:PublishingAndPrinting a owl:Class;
    rdfs:label "Publishing & Printing"@en,
        "Verlags- und Druckereiwesen"@de;
    rdfs:definition "Herausgeber von Zeitungen, Zeitschriften und Büchern, und Anbieter von Informationen in gedruckter oder elektronischer Form, sowie Dienstleister in diesem Gebiet (einschließlich Bildungseinrichtungen)"@de,
        "Publishers of newspapers, magazines and books, and providers of printed or electronic information; including providers of related services (including educational institutions)"@en;
    rdfs:subClassOf dax:Media .

dax:ReInsurance a owl:Class;
    rdfs:label "Re-Insurance"@en,
        "Rückversicherungen"@de;
    rdfs:definition "Companies mainly underwriting reinsurance cover"@en,
        "Unternehmen, die hauptsachlich Rückversicherungen abschließen"@de;
    rdfs:subClassOf dax:Insurance .

dax:RealEstate a owl:Class;
    rdfs:label "Immobilien"@de,
        "Real Estate"@en;
    rdfs:definition "Companies investing in property/real estate (directly or indirectly)"@en,
        "Gesellschaften, die direkt oder indirekt in Immobilien investieren"@de;
    rdfs:subClassOf dax:FinancialServices .

dax:RenewableEnergies a owl:Class;
    rdfs:label "Erneuerbare Energien"@de,
        "Renewable Energies"@en;
    rdfs:definition "Companies developing equipment for alternative and/or renewable energy generation, such as solar technology or wind-powered turbines"@en,
        "Unternehmen, die im Bereich der Entwicklung von Anlagen für alternative und/oder erneuerbare Energien (Solartechnik, Windkraftanlagen etc.) tätig sind"@de;
    rdfs:subClassOf dax:Industrial .

dax:RetailCatalog a owl:Class;
    rdfs:label "Retail, Catalog"@en,
        "Versandhandel"@de;
    rdfs:definition "Retailers selling mainly by mail order, which are not categorized under \"Retail, Internet\""@en,
        "Unternehmen des Einzelhandels, die ihre Güter vorwiegend über Versand anbieten, und nicht zur Branche \"Internethandel\" gehören"@de;
    rdfs:subClassOf dax:Retail .

dax:RetailFoodAndDrug a owl:Class;
    rdfs:label "Lebensmittel- und Drogeriemärkte, Apotheken"@de,
        "Retail, Food & Drug"@en;
    rdfs:definition "Besitzer und Betreiber von Lebensmittelgeschäften, Apotheken und Drogerien (Groß- und Einzelhandel)"@de,
        "Owners/operators of food stores, pharmacies and drugstores (wholesale and retail)"@en;
    rdfs:subClassOf dax:Retail .

dax:RetailInternet a owl:Class;
    rdfs:label "Internethandel"@de,
        "Retail, Internet"@en;
    rdfs:definition "Retailers selling their goods or services mainly over the Internet"@en,
        "Unternehmen des Einzelhandels, die ihre Güter oder Services vorwiegend über das Internet vertreiben"@de;
    rdfs:subClassOf dax:Retail .

dax:RetailMultiline a owl:Class;
    rdfs:label "Kaufhäuser"@de,
        "Retail, Multiline"@en;
    rdfs:definition "Retail companies with a broad product range (department stores)"@en,
        "Unternehmen des Einzelhandels mit einem breitgefächerten Produktangebot (Kaufhäuser)"@de;
    rdfs:subClassOf dax:Retail .

dax:RetailSpecialty a owl:Class;
    rdfs:label "Retail, Specialty"@en,
        "Spezialisierter Einzelhandel"@de;
    rdfs:definition "Retail companies with a very specific product range (such as fashion, electronics, etc.)"@en,
        "Unternehmen des Einzelhandels mit einem sehr speziellen Produktangebot (z. B. Mode und Elektronik)"@de;
    rdfs:subClassOf dax:Retail .

dax:SecuritiesBrokers a owl:Class;
    rdfs:label "Securities Brokers"@en,
        "Wertpapierhandel"@de;
    rdfs:definition "Companies active in the securities business, such as brokers, online banks, exchanges, etc."@en,
        "Unternehmen, die im Wertpapiergeschäft tätig sind wie Broker, Online-Banken, Börsen u. a."@de;
    rdfs:subClassOf dax:FinancialServices .

dax:Semiconductors a owl:Class;
    rdfs:label "Halbleiterprodukte"@de,
        "Semiconductors"@en;
    rdfs:definition "Hersteller und Entwickler von Halbleitern und ähnlichen Produkten"@de,
        "Producers and developers of semiconductors or similar products"@en;
    rdfs:subClassOf dax:Technology .

dax:SteelAndOtherMetals a owl:Class;
    rdfs:label "Stahl & Metalle"@de,
        "Steel & Other Metals"@en;
    rdfs:definition "Producers of steel or related in-process products; producers of iron, non-iron metals or precious metals"@en,
        "Unternehmen, die Stahl und Stahlzwischenprodukte herstellen, sowie Erzeuger von Eisen-, Nicht-Eisen- und Edelmetallen"@de;
