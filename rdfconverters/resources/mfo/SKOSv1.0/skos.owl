<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns="http://www.dfki.de/lt/skos.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.dfki.de/lt/skos.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >(C) Hans-Ulrich Krieger, DFKI-LT</rdfs:comment>
  </owl:Ontology>
  <owl:ObjectProperty rdf:ID="exactMatch">
    <owl:inverseOf rdf:resource="#exactMatch"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="closeMatch"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="narrowMatch">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="broadMatch"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="mappingRelation"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#closeMatch">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#mappingRelation"/>
    </rdfs:subPropertyOf>
    <owl:inverseOf rdf:resource="#closeMatch"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#mappingRelation">
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#broadMatch">
    <owl:inverseOf rdf:resource="#narrowMatch"/>
    <rdfs:subPropertyOf rdf:resource="#mappingRelation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="relatedMatch">
    <rdfs:subPropertyOf rdf:resource="#mappingRelation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <owl:inverseOf rdf:resource="#relatedMatch"/>
  </owl:ObjectProperty>
  <owl:AnnotationProperty rdf:ID="hiddenLabel"/>
  <owl:AnnotationProperty rdf:ID="prefLabel"/>
  <owl:AnnotationProperty rdf:ID="altLabel"/>
</rdf:RDF>

