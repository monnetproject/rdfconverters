    </rdfs:subClassOf>
    <rdfs:label xml:lang="de">Erdgas, Wasser und kombinierte Energieerzeugung</rdfs:label>
    <rdfs:label xml:lang="es">Gas, agua y servicios múltiples</rdfs:label>
    <rdfs:label xml:lang="en">Gas, Water &amp; Multi-utilities</rdfs:label>
