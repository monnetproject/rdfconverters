    <rdfs:label xml:lang="es">Software y serviciosinformáticos</rdfs:label>
    <rdfs:label xml:lang="en">Software &amp; Computer Services</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="ICB5557">
    <rdfs:definition xml:lang="de">Herausgeber von Informationen über Print- oder elektronische Medien.</rdfs:definition>
    <rdfs:definition xml:lang="es">Editores de información a través de medios impresoso electrónicos.</rdfs:definition>
    <rdfs:definition xml:lang="en">Publishers of information via printed or electronic media.</rdfs:definition>
