@prefix icb: <http://www.dfki.de/lt/icb.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.dfki.de/lt/icb.owl> a owl:Ontology;
    rdfs:comment "(C) Hans-Ulrich Krieger, DFKI-LT"^^xsd:string .

icb:ICB0533 a owl:Class;
    rdfs:label "Exploración y producción"@es,
        "Exploratiedoeleinden & Production"@nl,
        "Exploration & Production"@en,
        "Exploration und Produktion"@de;
    icb:code "0533";
    rdfs:definition "Bedrijven die actief zijn in de exploratie en winning van, de productie, de raffinage en het aanbod van olie en gas."@nl,
        "Companies engaged in the exploration for and drilling, production, refining and supply of oil and gas products."@en,
        "Empresas dedicadas a la exploración y extracción, producción,refinación y abastecimiento de productos petroleros y de gas."@es,
        "Unternehmen, die in der Erdöl- und Erdgasexploration tätig sind (Bohrung, Produktion, Raffinerierung von und Versorgung mit Erdöl- und Erdgasprodukten)."@de;
    rdfs:subClassOf icb:ICB0530 .

icb:ICB0537 a owl:Class;
    rdfs:label "Geïntegreerde Oliehoudende & Identificatie"@nl,
        "Integrated Oil & Gas"@en,
        "Integrierte Erdöl- und Erdgasgesellschaften"@de,
        "Petróleo y gas integrado"@es;
    icb:code "0537";
    rdfs:definition "Empresas integradas de petróleo y gas dedicadas a la exploracióny extracción, producción, refinación, distribución y ventasminoristas de productos petroleros y de gas."@es,
        "Geïntegreerde olie en gas bedrijven die actief zijn in de exploratie en winning van, de productie, de raffinage, distributie en detailhandelverkoop van olie en gas."@nl,
        "Integrated oil and gas companies engaged in the exploration for and drilling, production, refining, distribution and retail sales of oil and gas products."@en,
        "Mit der Suche nach und der Bohrung, Produktion und Raffinierung sowie dem Vertrieb und Einzelhandelsverkauf von Öl und Gasprodukten befasste integrierte Öl- und Gasgesellschaften."@de;
    rdfs:subClassOf icb:ICB0530 .

icb:ICB0573 a owl:Class;
    rdfs:label "Ausrüstung und Dienstleistungen"@de,
        "Equipos y servicios petroleros"@es,
        "Oil Equipment & Services"@en,
        "Olie Uitrusting & intesa"@nl;
    icb:code "0573";
    rdfs:definition "Anbieter von Ausrüstung und Dienstleistungen für Ölfelder und Offshore-Plattformen (Bohrung, Exploration, Erdbebeninformationen und Plattformbau)."@de,
        "De leveranciers van apparatuur en diensten olievelden en offshore platforms, zoals proefboringen, exploratiedoeleinden, seismische-informatiediensten en webplatform Constructiewijze."@nl,
        "Proveedores de equipos y servicios para yacimientos petroleros yplataformas marinas, tales como extracción, exploración,servicios de información sísmica y construcción de plataformas."@es,
        "Suppliers of equipment and services to oil fields and offshore platforms, such as drilling, exploration, seismic-information services and platform construction."@en;
    rdfs:subClassOf icb:ICB0570 .

icb:ICB0577 a owl:Class;
    rdfs:label "Ductos"@es,
        "Pipelines"@de,
        "Pipelines"@en,
        "Transportleiding"@nl;
    icb:code "0577";
    rdfs:definition "Betreiber von Pipelines für den Transport von Erdöl, Erdgas und anderen Brennstoffarten. Ausgeschlossen sind Pipeline- Betreiber, die ihren Umsatz hauptsächlich aus dem Direktverkauf an Endverbraucher erzielen (siehe: Gasversorgung)."@de,
        "Exploitanten van transportleiding lastdragende olie, gas of andere vormen van brandstof. Sluit pijpleiding Operators die het grootste deel van hun inkomsten uit de directe verkoop aan eindgebruikers, die zijn ingedeeld in Identificatie Distribution."@nl,
        "Operadores de ductos para el transporte de petróleo, gasu otras formas de combustible. Excluye los operadores deductos que obtienen la mayor parte de sus ingresos de lasventas directas a usuarios finales, los que están clasificadosbajo Distribución de gas."@es,
        "Operators of pipelines carrying oil, gas or other forms of fuel. Excludes pipeline operators that derive the majority of their revenues from direct sales to end users, which are classified under Gas Distribution."@en;
    rdfs:subClassOf icb:ICB0570 .

icb:ICB0583 a owl:Class;
    rdfs:label "Equipos de energías renovables"@es,
        "Erneuerbare Energieanlagen"@de,
        "Hernieuwbare energie Uitrusting"@nl,
        "Renewable Energy Equipment"@en;
    icb:code "0583";
    rdfs:definition "Bedrijven die ontwikkelen of vervaardigings- stromingsenergie equipment gebruikmaken van de bronnen, zoals zonne-energie, windenergie, getijdenenergie, geothermische, waterkrachtcapaciteit en golven."@nl,
        "Companies that develop or manufacture renewable energy equipment utilizing sources such as solar, wind, tidal, geothermal, hydro and waves."@en,
        "Empresas que desarrollan o fabrican equipos de energíasrenovables, los cuales utilizan fuentes como la energía solar,eólica, mareomotriz, geotérmica, hídrica y de las olas."@es,
        "Firmen, die erneuerbare, auf Solar-, Wind-, Gezeiten-, Wasser- und Wellenkraft sowie geothermischer Energie beruhende Anlagen entwickeln oder herstellen."@de;
    rdfs:subClassOf icb:ICB0580 .

icb:ICB0587 a owl:Class;
    rdfs:label "Alternatieve brandstoffen"@nl,
        "Alternative Fuels"@en,
        "Alternative Kraftstoffe"@de,
        "Combustibles alternativos"@es;
    icb:code "0587";
    rdfs:definition "Bedrijven die alternatieve brandstoffen zoals ethanol, methanol, waterstof en biobrandstoffen die hoofdzakelijk worden gebruikt om de macht van de voertuigen en bedrijven die betrokken zijn bij de productie van wegvoertuig brandstofcellen en / of de ontwikkeling van alternatieve aan infrastructuur."@nl,
        "Companies that produce alternative fuels such as ethanol, methanol, hydrogen and bio-fuels that are mainly used to power vehicles, and companies that are involved in the production of vehicle fuel cells and/or the development of alternative fuelling infrastructure."@en,
        "Empresas que producen combustibles alternativos, comoetanol, metanol, hidrógeno y biocombustibles, que se utilizanprincipalmente en vehículos (Strichpunkt) y empresas que se dedican ala producción de celdas de combustible para vehículos y aldesarrollo de infraestructura para combustibles alternativos."@es,
        "Firmen, die alternative Kraftstoffe wie Ethanol, Methanol, Wasserstoff und Bio-Kraftstoffe produzieren, die hauptsächlich zum Antrieb von Fahrzeugen benutzt werden und Firmen, die mit der Produktion von Kraftstoffzellen und/ oder mit der Entwicklung der Infrastruktur für alternative Kraftstoffe befasst sind."@de;
    rdfs:subClassOf icb:ICB0580 .

icb:ICB1353 a owl:Class;
    rdfs:label "Commodity Chemicals"@en,
        "Gebrauchschemikalien"@de,
        "Grondstoffenontwikkeling rubberchemicaliën"@nl,
        "Productos químicos básicos"@es;
    icb:code "1353";
    rdfs:comment ""^^xsd:string;
    rdfs:definition "Erzeuger und Händler von einfachen chemischen Produkten, die in erster Linie für die Herstellung komplexer Chemikalien oder Produkte genutzt werden. Dazu zählen Kunststoffe und Gummi in ihrer Rohform, Glasfasern und Synthetikfasern."@de,
        "Producenten en distributeurs van eenvoudige chemische producten die hoofdzakelijk worden gebruikt om formulate complexer chemicals of producten, waaronder plastic- en siliconenrubber in hun raw vorm, glasvezel en synthetische vezels."@nl,
        "Producers and distributors of simple chemical products that are primarily used to formulate more complex chemicals or products, including plastics and rubber in their raw form, fibreglass and synthetic fibres."@en,
        "Productores y distribuidores de productos químicos simples queson usados principalmente para formular químicos o productosmás complejos, lo que incluye plásticos y goma en bruto, fibra devidrio y fibras sintéticas."@es;
    rdfs:subClassOf icb:ICB1350 .

icb:ICB1357 a owl:Class;
    rdfs:label "Productos químicosespecializados"@es,
        "Specialty Chemicals"@en,
        "Specialty rubberchemicaliën"@nl,
        "Spezielle Chemikalien"@de;
    icb:code "1357";
    rdfs:definition "Erzeuger und Händler von Fertigchemikalien für Industrie oder Endverbraucher, einschließlich Farben, zellulare Polymere, Beschichtungen, Spezialkunststoffe und andere Chemikalien für spezielle Anwendungen. Dazu zählen Hersteller von Farb-, Aroma- und Duftstoffen, Düngemitteln, Pestiziden, Chemikalien für die Medikamentenherstellung, Farbe in pigmentierter Form und Glas in unbearbeiteter Form. Ausgeschlossen sind Hersteller von Farb- und Glasprodukten für das Baugewerbe (siehe: Baustoffe und Befestigungen)."@de,
        "Producenten en distributeurs van nabewerken chemicals voor industrieën en eindgebruikers, waaronder tannine Gevulcaniseerde polymeren, beschermlagen, speciale plastic- en andere chemische stoffen voor gespecialiseerde certificaataanvragen. Ook bereiders van kleurstoffen, smaak en geurstoffen, meststoffen en pesticiden, chemische stoffen die worden gebruikt om drugs, verf in haar pigment vorm en glass in haar vorm. Dat producenten van verf en glass producten die gebruikt worden voor de bouw, die zijn ingedeeld in Voortbouwend Materials & Vaste inrichting."@nl,
        "Producers and distributors of finished chemicals for industries or end users, including dyes, cellular polymers, coatings, special plastics and other chemicals for specialized applications. Includes makers of colourings, flavours and fragrances, fertilizers, pesticides, chemicals used to make drugs, paint in its pigment form and glass in its unfinished form. Excludes producers of paint and glass products used for construction, which are classified under Building Materials & Fixtures."@en,
        "Productores y distribuidores de productos químicos terminadospara industrias o usuarios finales, incluidas tinturas, polímeroscelulares, revestimientos de protección, plásticos especialesy otros productos químicos para aplicaciones especializadas.Incluye a los fabricantes de colorantes, saborizantes y fragancias,fertilizantes, pesticidas, productos químicos utilizadospara fabricar medicamentos, pintura en su forma de pigmentoy vidrio en su forma no terminada. Excluye productores depintura y productos de vidrio utilizados en la construcción, quese encuentran clasificados bajo Materiales de construcción einstalaciones fijas."@es;
    rdfs:subClassOf icb:ICB1350 .

icb:ICB1733 a owl:Class;
    rdfs:label "Bosbouwgronden"@nl,
        "Forestry"@en,
        "Forstwirtschaft"@de,
        "Silvicultura"@es;
    icb:code "1733";
    rdfs:definition "De eigenaren en exploitanten van hout geschriften, bosvarken tree boomkwekerijproducten en sawmills. Sluit aanbieders van nabewerken houtprodukten zoals houten bomen, die zijn ingedeeld in Voortbouwend Materials & Vaste inrichting."@nl,
        "Eigentümer und Betreiber von Nutzwaldflächen, Baumschulen und Sägemühlen. Ausgeschlossen sind Lieferanten von Holzfertigprodukten, wie Holzbalken (siehe: Baustoffe und Befestigungen)."@de,
        "Owners and operators of timber tracts, forest tree nurseries and sawmills. Excludes providers of finished wood products such as wooden beams, which are classified under Building Materials & Fixtures."@en,
        "Propietarios y operadores de plantaciones forestales, viverosde árboles forestales y aserraderos. Excluye proveedores deproductos de madera terminados tales como vigas de madera,que están clasificadas bajo Materiales de construcción einstalaciones fijas."@es;
    rdfs:subClassOf icb:ICB1730 .

icb:ICB1737 a owl:Class;
    rdfs:label "Papel"@es,
        "Paper"@en,
        "Papierindustrie"@de,
        "Ref"@nl;
    icb:code "1737";
    rdfs:comment ""^^xsd:string;
    rdfs:definition "Hersteller, Verarbeiter, Händler und Vertriebshändler für alle Papiersorten. Ausgeschlossen sind Hersteller von gedruckten Produkten (siehe: Business Support Services) sowie Hersteller von Papierprodukten, wie z.B. Pappbecher und Papierservietten (siehe: Kurzlebige Haushaltwaren)."@de,
        "Producenten, converters, Merchants en distributeurs van alle handelsklassen papier. Sluit bereiders van geguillocheerde vormen, die zijn ingedeeld in Zakelijke Ondersteunende diensten, en fabrikanten van onderdelen zoals cups en napkins, die zijn ingedeeld in Nondurable Household Produkten."@nl,
        "Producers, converters, merchants and distributors of all grades of paper. Excludes makers of printed forms, which are classified under Business Support Services, and manufacturers of paper items such as cups and napkins, which are classified under Nondurable Household Products."@en,
        "Productores, convertidores, comerciantes y distribuidores depapeles de todos los grados. Excluye fabricantes de formulariosimpresos, que están clasificados bajo Servicios de apoyoempresarial, y fabricantes de artículos de papel como vasosy servilletas, que están clasificados bajo Artículos para el hogarno duraderos."@es;
    rdfs:subClassOf icb:ICB1730 .

icb:ICB1753 a owl:Class;
    rdfs:label "Aluminio"@es,
        "Aluminium"@de,
        "Aluminium"@en,
        "Aluminiumcoating"@nl;
    icb:code "1753";
    rdfs:definition "Bedrijven die mijn of proces bauxiet of de productie en distributie van aluminiumcoating staven, staven en andere producten voor gebruik door andere sectoren. Dat fabrikanten van nabewerken aluminiumproducten, zoals de kant kiezen, die zijn ingedeeld volgens de aard van het eindproduct."@nl,
        "Companies that mine or process bauxite or manufacture and distribute aluminium bars, rods and other products for use by other industries. Excludes manufacturers of finished aluminium products, such as siding, which are categorized according to the type of end product."@en,
        "Empresas que extraen o procesan bauxita o fabrican ydistribuyen barras, varillas y otros productos para su uso enotras industrias. Excluye a fabricantes de productos de aluminioterminados, tales como revestimientos, que se clasificansegún el tipo de producto final."@es,
        "Unternehmen, die Bauxit abbauen oder verarbeiten sowie Aluminiumträger, -stangen oder andere Produkte für die Nutzung in anderen Industriezweigen herstellen oder vertreiben. Ausgeschlossen sind Hersteller von Fertigprodukten aus Aluminium, z.B. Gleise, die in der Kategorie des jeweiligen Endprodukts aufgeführt sind."@de;
    rdfs:subClassOf icb:ICB1750 .

icb:ICB1755 a owl:Class;
    rdfs:label "Metales no ferrosos"@es,
        "Nichteisenhaltige Metalle"@de,
        "Nonferrous Metals"@en,
        "Nonferrous Metals"@nl;
    icb:code "1755";
    rdfs:definition "Producenten en handelaars van metalen en primary metaalsoort andere producten dan ijzer en staal, aluminium. Sluit bedrijven die de eindproducten die zijn ingedeeld volgens de aard van het eindproduct."@nl,
        "Producers and traders of metals and primary metal products other than iron, aluminium and steel. Excludes companies that make finished products, which are categorized according to the type of end product."@en,
        "Productores y comerciantes de metales y productos metálicosprimarios, excluidos el hierro, el aluminio y el acero. Excluye aempresas que fabrican productos terminados, que se clasificansegún el tipo de producto final."@es,
        "Produzenten und Händler von Metallen und Neumetallen mit Ausnahme von Eisen, Aluminium und Stahl. Ausgeschlossen sind Hersteller von Fertigprodukten: diese werden nach Endprodukttyp klassifiziert."@de;
    rdfs:subClassOf icb:ICB1750 .

icb:ICB1757 a owl:Class;
    rdfs:label "Eisen und Stahl"@de,
        "Hierro y acero"@es,
        "Ijzer en staal"@nl,
        "Iron & Steel"@en;
    icb:code "1757";
    rdfs:definition "Fabricantes y accionistas de productos primarios de hierro yacero, como tubos, alambres, láminas y barras, incluidos todoslos procesos desde la fundición en altos hornos hasta laminadorasy fundiciones. Incluye empresas que extraen principalmenteminerales de hierro."@es,
        "Fabrikanten en stockholders van primary ijzer en staal producten zoals buizen, magneetdraad, hoeslakens en bars, waarin alle processen van omsmelting in hoogovens van warmwalsen en gieterijen. Ook bedrijven die primarily mijnbestrijdingsoutillage ijzererts."@nl,
        "Hersteller und Aktionäre von primären Eisen- und Stahlprodukten wie z.B. Rohre, Leitungsdrähte, Blechteile und Eisenbarren, die alle Prozesse von der Schmelze in Hochöfen bis zu den Walzwerken und Gießereien umfassen. Einschließlich Firmen, deren Hauptgeschäft der Abbau von Eisenerz ist."@de,
        "Manufacturers and stockholders of primary iron and steel products such as pipes, wires, sheets and bars, encompassing all processes from smelting in blast furnaces to rolling mills and foundries. Includes companies that primarily mine iron ores."@en;
    rdfs:subClassOf icb:ICB1750 .

icb:ICB1771 a owl:Class;
    rdfs:label "Carbón"@es,
        "Coal"@en,
        "Kohle"@de,
        "Steenkool"@nl;
    icb:code "1771";
    rdfs:definition "Bedrijven die actief zijn in de exploratie en winning van steenkool."@nl,
        "Companies engaged in the exploration for or mining of coal."@en,
        "Empresas dedicadas a la exploración o extracción de carbón."@es,
        "Unternehmen, die in der Kohleexploration oder -gewinnung tätig sind."@de;
    rdfs:subClassOf icb:ICB1770 .

icb:ICB1773 a owl:Class;
    rdfs:label "Diamanten & edelstenen"@nl,
        "Diamanten und Edelsteine"@de,
        "Diamantes y piedras preciosas"@es,
        "Diamonds & Gemstones"@en;
    icb:code "1773";
    rdfs:comment ""^^xsd:string;
    rdfs:definition "Bedrijven die actief zijn in de exploratie en de productie van diamanten en andere edelstenen."@nl,
        "Companies engaged in the exploration for and production of diamonds and other gemstones."@en,
        "Empresas dedicadas a la exploración y producción de diamantesy otras piedras preciosas."@es,
        "Unternehmen, die in der Exploration und Gewinnung von Diamanten und anderen Edelsteinen tätig sind."@de;
    rdfs:subClassOf icb:ICB1770 .

icb:ICB1775 a owl:Class;
    rdfs:label "Algemene Mijnbouw"@nl,
        "Allgemeiner Bergbau"@de,
        "General Mining"@en,
        "Minería general"@es;
    icb:code "1775";
    rdfs:comment ""^^xsd:string;
    rdfs:definition "Bedrijven die werkzaam zijn in de exploratie en winning of refining van mineralen niet gedefinieerd in de Mijnbouw sector."@nl,
        "Companies engaged in the exploration, extraction or refining of minerals not defined elsewhere within the Mining sector."@en,
        "Empresas dedicadas a la exploración, extracción o refinaciónde minerales no definidos en otras categorías dentro del sectorMinería."@es,
        "Unternehmen, die in der Exploration, Gewinnung oder Veredelung von Mineralien tätig und in keinem anderen Bereich unter Bergbau definiert sind."@de;
    rdfs:subClassOf icb:ICB1770 .

icb:ICB1777 a owl:Class;
    rdfs:label "Gold"@de,
        "Gold Mining"@en,
        "Goudtransacties Mijnbouw"@nl,
        "Minería de oro"@es;
    icb:code "1777";
    rdfs:definition "Exploradores y extractores o refinadores de minerales auríferos."@es,
        "Prospectors en extractors of de raffinaderijen van goudtransacties-conische ertsen."@nl,
        "Prospectors for and extractors or refiners of gold-bearing ores."@en,
        "Prospektoren, Abbauer oder Veredler von goldhaltigen Erzen."@de;
    rdfs:subClassOf icb:ICB1770 .

icb:ICB1779 a owl:Class;
    rdfs:label "Platin und Edelmetalle"@de,
        "Platino y metales preciosos"@es,
        "Platinum & Precious Metals"@en,
        "Platinum & edele Metals"@nl;
    icb:code "1779";
    rdfs:definition "Bedrijven die actief zijn in de exploratie en de productie van platinum, zilver en andere edele metalen niet gedefinieerd."@nl,
        "Companies engaged in the exploration for and production of platinum, silver and other precious metals not defined elsewhere."@en,
        "Empresas dedicadas a la exploración y producción de platino,plata y otros metales preciosos no definidos en otras categorías."@es,
        "Unternehmen, die in der Exploration und Produktion von Platin, Silber und anderen Edelmetallen tätig und nicht an anderer Stelle definiert sind."@de;
    rdfs:subClassOf icb:ICB1770 .

icb:ICB2353 a owl:Class;
    rdfs:label "Baustoffe und Befestigungen"@de,
        "Building Materials & Fixtures"@en,
        "Materiales de construcción einstalaciones fijas"@es,
        "Voorbouwt Materials & Vaste inrichting"@nl;
    icb:code "2353";
    rdfs:definition "De producenten van grondstoffen in de bouw en de renovatie van gebouwen en structuren, met inbegrip van cement en andere toeslagmaterialen, houten balken en frames, verf, glas, dakbedekkingen en vloeren andere materialen dan tapijten. Ook de producenten van de badkamer en keuken ontmoetingen, sanitair bedrijfsbenodigdheden en centrale klimaatregelings- en verwarmingsapparaten. Sluit bedrijfsproducenten raw algemeen, die zijn ingedeeld in de bosbouw."@nl,
        "Hersteller von Materialien, die für Bau und Sanierung von Gebäuden und Komplexen genutzt werden. Dazu zählen Zement und andere Zuschlagstoffe, Holzbalken und Rahmen, Farbe, Glas, Dach- und Fußbodenmaterial mit Ausnahme von Teppichboden. Einschließlich der Hersteller von Badezimmer- und Küchenbefestigungen, Klempnermaterial sowie Elementen für Klima- und Heizungsanlagen. Ausgeschlossen sind Rohholzproduzenten (siehe: Fortwirtschaft)."@de,
        "Producers of materials used in the construction and refurbishment of buildings and structures, including cement and other aggregates, wooden beams and frames, paint, glass, roofing and flooring materials other than carpets. Includes producers of bathroom and kitchen fixtures, plumbing supplies and central air-conditioning and heating equipment. Excludes producers of raw lumber, which are classified under Forestry."@en,
        "Productores de materiales usados en la construcción yrenovación de edificios y estructuras, incluidos el cementoy otros agregados, vigas y marcos de madera, pintura, vidrio,materiales para techos y pisos, excluidas las alfombras. Incluyeproductores de instalaciones fijas de baño y cocina, materialesde plomería y equipos de aire acondicionado y calefaccióncentral. Excluye productores de madera en bruto, que estánclasificados bajo Silvicultura."@es;
    rdfs:subClassOf icb:ICB2350 .

icb:ICB2357 a owl:Class;
    rdfs:label "Construcción pesada"@es,
        "Heavy Construction"@en,
        "Hoch- und Tiefbau"@de,
        "Zware Construction"@nl;
    icb:code "2357";
    rdfs:definition "Bedrijven die betrokken zijn bij de bouw van gebouwen, infrastructuur, wegen en bruggen, residentieel appartement buildings en aanbieders van diensten te bedrijven, zoals de architecten, masons, loodgieters en elektrotechnisch aannemingsbedrijven."@nl,
        "Companies engaged in the construction of commercial buildings, infrastructure such as roads and bridges, residential apartment buildings, and providers of services to construction companies, such as architects, masons, plumbers and electrical contractors."@en,
        "Empresas dedicadas a la construcción de edificios comerciales,infraestructura del tipo de caminos y puentes, edificios deapartamentos residenciales y proveedores de servicios paraempresas de construcción, tales como arquitectos, albañiles,plomeros y contratistas eléctricos."@es,
        "Unternehmen, die im Bau von Geschäftsgebäuden und der Schaffung von Infrastrukturen wie Straßen, Brücken und Wohngebäuden tätig sind, sowie Unternehmen, die Dienstleistungen für Baufirmen anbieten, z.B. Architekten, Handwerker, Klempner und Elektriker."@de;
    rdfs:subClassOf icb:ICB2350 .

icb:ICB2713 a owl:Class;
    rdfs:label "Aeroespacial"@es,
        "Aerospace"@en,
        "De ruimtevaart"@nl,
        "Luftfahrt"@de;
    icb:code "2713";
    rdfs:comment ""^^xsd:string;
    rdfs:definition "Fabricantes, ensambladores y distribuidores de aviones ypartes de aviones utilizadas principalmente en el transporteaéreo comercial y privado. Excluye a fabricantes de satélitesde comunicaciones, que están clasificados bajo Equipos detelecomunicaciones."@es,
        "Fabrikanten, assembleurs en distributeurs van vliegtuigen en vliegtuigonderdelen primarily gebruikt in commerciële of particuliere luchtvervoer. Sluit fabrikanten van gegevenstoezending satellieten, die zijn ingedeeld in Telephone Uitrusting."@nl,
        "Hersteller, Montagefirmen und Händler von Flugzeugen und Flugzeugteilen, die in erster Linie im kommerziellen oder privaten Lufttransport eingesetzt werden. Ausgeschlossen sind Hersteller von Kommunikationssatelliten (siehe: Telekommunikationsausrüstung)."@de,
        "Manufacturers, assemblers and distributors of aircraft and aircraft parts primarily used in commercial or private air transport. Excludes manufacturers of communications satellites, which are classified under Telecommunications Equipment."@en;
    rdfs:subClassOf icb:ICB2710 .

icb:ICB2717 a owl:Class;
    rdfs:label "Defensa"@es,
        "Defense"@en,
        "Defense"@nl,
        "Verteidigung"@de;
    icb:code "2717";
    rdfs:definition "De producenten van onderdelen en apparatuur voor de defensie-industrie, met inbegrip van militaire vliegtuigen, Radarapparatuur en wapens."@nl,
        "Hersteller von Bauteilen und Ausrüstung für die Verteidigungsindustrie, einschließlich Militärflugzeuge, Radarausrüstung und Waffen."@de,
        "Producers of components and equipment for the defense industry, including military aircraft, radar equipment and weapons."@en,
        "Productores de componentes y equipo para la industria de ladefensa, incluidos los aviones militares, equipos de radary armas."@es;
    rdfs:subClassOf icb:ICB2710 .

icb:ICB2723 a owl:Class;
    rdfs:label "Benzinetanks & Packaging"@nl,
        "Container und Verpackungen"@de,
        "Containers & Packaging"@en,
        "Envases y embalajes"@es;
    icb:code "2723";
    rdfs:definition "Fabricantes y distribuidores de cajas de cartón, bolsas, cajas, latas,tambores, botellas y frascos, y vidrio usados para embalaje."@es,
        "Fabrikanten en distributeurs van karton, bags, kartons fusten, flessen, blikjes, en potten en glass gebruikt voor verpakkingen."@nl,
        "Hersteller und Händler von Verpackungsmaterialien wie Kartons, Taschen, Schac, Schachteln, Dosen, Fässern, Flaschen, Gläsern und Glas als Verpackungsmaterial."@de,
        "Makers and distributors of cardboard, bags, boxes, cans, drums, bottles and jars and glass used for packaging."@en;
    rdfs:subClassOf icb:ICB2720 .

icb:ICB2727 a owl:Class;
    rdfs:label "Actividad industrial diversificada"@es,
        "Diversified Industrials"@en,
        "Diversifizierte Gewerbe"@de,
        "Gediversifieerde Industrials"@nl;
    icb:code "2727";
    rdfs:definition "Empresas industriales dedicadas a tres o más clases denegocios dentro del sector industrial que difieren de manerasustancial entre sí."@es,
        "Industrial companies engaged in three or more classes of business within the Industrial industry that differ substantially from each other."@en,
        "Industrieunternehmen, die in drei oder mehr, verschiedenen Geschäftszweigen innerhalb einer Branche tätig sind."@de,
        "Industriële bedrijven die zich bezighouden met drie of meer klassen van de werkzaamheden binnen de industriële sector die sterk van elkaar verschillen."@nl;
    rdfs:subClassOf icb:ICB2720 .

icb:ICB2733 a owl:Class;
    rdfs:label "Componentes y equiposeléctricos"@es,
        "Electrical Components & Equipment"@en,
        "Elektrische Bauteile und Ausrüstungselemente"@de,
        "Elektrotechnisch Onderdelen & Uitrusting"@nl;
    icb:code "2733";
    rdfs:definition "Fabricantes y distribuidores de partes eléctricas de productosterminados, como las tarjetas de circuitos impresos para radios,televisores y otros bienes de consumo electrónicos. Incluyefabricantes de cables, alambres, cerámica, transistores, adaptadoreseléctricos, pilas de combustible y cámaras de seguridad."@es,
        "Fabrikanten en distributeurs van elektrotechnisch interventieonderdelen voor afgewerkte producten, zoals geguillocheerde Circuit trawlborden voor radio, televisie en andere consumentenelektronika. Ook bereiders van cables, magneetdraad, keramiek, transistors, elektrische adapters vormen en de veiligheidscamera 's."@nl,
        "Hersteller und Händler von elektrischen Bauteilen für Endprodukte, Ausrüstungselemente z.B. gedruckte Leiterplatten für Radios, Fernseher und andere Elektronikgeräte. Dazu zählen zudem Hersteller von Kabeln, Drähten, Keramikkondensatoren, Transistoren, elektrischen Adaptern, Brennstoffzellen und Sicherheitskameras."@de,
        "Makers and distributors of electrical parts for finished products, such as printed circuit boards for radios, televisions and other consumer electronics. Includes makers of cables, wires, ceramics, transistors, electric adapters and security cameras."@en;
    rdfs:subClassOf icb:ICB2730 .

icb:ICB2737 a owl:Class;
    rdfs:label "Electronic Equipment"@en,
        "Elektronische Ausrüstungselemente"@de,
        "Elektronische Uitrusting"@nl,
        "Equipos electrónicos"@es;
    icb:code "2737";
    rdfs:definition "Fabricantes y distribuidores de productos electrónicos usadosen distintas industrias. Incluye fabricantes de láseres, tarjetasinteligentes, lectores de código de barras, lectores de huellasdigitales y otros equipos de fábrica electrónicos."@es,
        "Fabrikanten en distributeurs van elektronische producten gebruikt in verschillende sectoren. Ook bereiders van laserwapens, slimme kaarten bedieningsstang scanners, vingerafdrukken equipment productiecontrolesysteem en andere elektronische apparatuur."@nl,
        "Hersteller und Händler von elektronischen Produkten, die in verschiedenen Industriezweigen genutzt werden. Dazu zählen u.a. Hersteller von Laser-, Chipkarten- und Barcodescanner, Ausrüstung zur Erkennung von Fingerabdrücken und anderen elektronischen Werksausrüstungen."@de,
        "Manufacturers and distributors of electronic products used in different industries. Includes makers of lasers, smart cards, bar scanners, fingerprinting equipment and other electronic factory equipment."@en;
    rdfs:subClassOf icb:ICB2730 .

icb:ICB2753 a owl:Class;
    rdfs:label "Commercial Vehicles & Trucks"@en,
        "Commerciële Voertuigen & Trucks"@nl,
        "Nutzfahrzeuge und LKW"@de,
        "Vehículos y camionescomerciales"@es;
    icb:code "2753";
    rdfs:definition "Fabricantes y distribuidores de vehículos comerciales ymaquinaria pesada agrícola y para la construcción, lo queincluye vagones de ferrocarril, tractores, máquinas excavadoras,grúas, autobuses y cortadoras de césped industriales. Incluyeconstructores de buques no militares, tales como constructoresde buques para cruceros y transbordadores."@es,
        "Fabrikanten en distributeurs van commerciële voertuigen en zware landbouw- en bouwmachines, met inbegrip van het spoor auto 's, trekkers, bulldozers, hijskranen, bussen en industriële hand laadmachines. Ook de niet-militaire scheepsbouwbedrijven, zoals bouwlieren van passagiersschepen en veerboten."@nl,
        "Hersteller und Vertreiber von Nutzfahrzeugen und schweren landwirtschaftlichen Maschinen und Baumaschinen, einschließlich Schienenfahrzeuge, Traktoren, Planierraupen, Kräne, Busse und industrielle Rasenmäher. Einschließlich nichtmilitärische Schiffbauer wie z. B. Erbauer von Kreuzfahrtschiffen und Fähren."@de,
        "Manufacturers and distributors of commercial vehicles and heavy agricultural and construction machinery, including rail cars, tractors, bulldozers, cranes, buses and industrial lawn mowers. Includes non-military shipbuilders, such as builders of cruise ships and ferries."@en;
    rdfs:subClassOf icb:ICB2750 .

icb:ICB2757 a owl:Class;
    rdfs:label "Industrial Machinery"@en,
        "Industriemaschinen"@de,
        "Industriële Installaties"@nl,
        "Maquinaria industrial"@es;
    icb:code "2757";
    rdfs:definition "Designers, manufacturers, distributors and installers of industrial machinery and factory equipment, such as machine tools, lathes, presses and assembly line equipment. Includes makers of pollution control equipment, castings, pressings, welded shapes, structural steelwork, compressors, pumps, bearings, elevators and escalators."@en,
        "Diseñadores, fabricantes, distribuidores e instaladoresde maquinaria industrial y equipo de fábrica, tales comomáquinas herramienta, tornos, prensas y equipos de líneasde ensamblaje. Incluye fabricantes de equipos de control dela polución, fundición, prensado, uniones soldadas, aceroestructural, compresores, bombas, rodamientos, elevadoresy escaleras mecánicas."@es,
        "Entwickler, Hersteller und Händler von sowie Montagefirmen für Industriemaschinen und Werksausrüstungen, z.B. Werkzeugmaschinen, Drehmaschinen, Pressen und Montagestraßenausrüstung. Dazu zählen zudem Hersteller von Steuerausrüstung zur Regulierung der Schadstoffemission, Gussformen, Pressformen, geschmiedeten Formen, Strukturstahlarbeiten, Kompressoren, Pumpen, Lagern, Fahrstühlen und Rolltreppen."@de,
        "Ontwerpers, producenten, distributeurs en installateurs productiecontrolesysteem industriële machines en apparatuur, zoals gereedschapsmachines, lathes, dehydratiepersen en assemblage- bestelpost equipment. Ook bereiders van vervuiling Verkeersregelinstallaties, gietstukken pressings, gelast dierfiguren, structural steelwork, Compressoren, pompen, opleggingen, liften en roltrappen."@nl;
    rdfs:subClassOf icb:ICB2750 .

icb:ICB2771 a owl:Class;
    rdfs:label "Belevering intesa"@nl,
        "Delivery Services"@en,
        "Servicios de entrega"@es,
        "Versanddienst"@de;
    icb:code "2771";
    rdfs:definition "Betreiber von Post- und Paketlieferdiensten für Handel und Privatkunden. Dazu zählen zudem Zustell- und Logistikdienstleistungen, die hauptsächlich durch Lufttransport realisiert werden."@de,
        "Exploitanten van koeriersdienst en verpakkingseenheid belevering diensten voor commerciële en consument gebruik. Ook koerier- en logistieke diensten hoofdzakelijk met het luchtvervoer."@nl,
        "Operadores de servicios de correo y entrega de paquetes parauso comercial y de consumo. Incluye servicios de mensajería ylogística que involucran principalmente el transporte aéreo."@es,
        "Operators of mail and package delivery services for commercial and consumer use. Includes courier and logistic services primarily involving air transportation."@en;
    rdfs:subClassOf icb:ICB2770 .

icb:ICB2773 a owl:Class;
    rdfs:label "De mariene TRANSPORTATION"@nl,
        "Marine Transportation"@en,
        "Seetransport"@de,
        "Transporte marítimo"@es;
    icb:code "2773";
    rdfs:definition "Anbieter im kommerziellen Seefrachtversand, z.B. Containerversand. Ausgeschlossen sind Häfen (siehe: Transportdienstleistungen) und Werften (siehe: Nutzfahrzeuge und LKW)."@de,
        "De aanbieders van het vervoer voor commerciële markten, zoals bergingsmiddel scheepvaartlanden. Sluit havens, die zijn ingedeeld in TRANSPORTATION intesa en scheepsbouwbedrijven, die zijn ingedeeld in Zakelijk Voertuigen & Trucks."@nl,
        "Proveedores de transporte por agua para mercados comerciales,tales como transporte de contenedores. Excluye puertos, queestán clasificados bajo Servicios de transporte, y constructoresde buques, que están clasificados bajo Vehículos y camionescomerciales."@es,
        "Providers of on-water transportation for commercial markets, such as container shipping. Excludes ports, which are classified under Transportation Services, and shipbuilders, which are classified under Commercial Vehicles & Trucks."@en;
    rdfs:subClassOf icb:ICB2770 .

icb:ICB2775 a owl:Class;
    rdfs:label "Eisenbahn"@de,
        "Ferrocarriles"@es,
        "Railroads"@en,
        "Spoorwegnetten"@nl;
    icb:code "2775";
    rdfs:definition "Anbieter von Gütertransporten auf Schienen und Bahnlinien. Ausgeschlossen sind Bahngesellschaften für den Personentransport (siehe: Reisen und Tourismus) sowie Schienenfahrzeughersteller (siehe: Nutzfahrzeuge und LKW)."@de,
        "De aanbieders van industriële spoorvoorzieningen transportation en spoorlijnen. Sluit passenger spoorwegmaatschappijen, die zijn ingedeeld in Reis- & toerisme, en fabrikanten van auto 's, die zijn ingedeeld in Zakelijk Voertuigen & Trucks."@nl,
        "Proveedores de transporte ferroviario industrial y líneasferroviarias. Excluye empresas de ferrocarriles de pasajeros,que están clasificadas bajo Viajes y turismo, y fabricantesde vagones de trenes, que están clasificados bajo Vehículosy camiones comerciales."@es,
        "Providers of industrial railway transportation and railway lines. Excludes passenger railway companies, which are classified under Travel & Tourism, and manufacturers of rail cars, which are classified under Commercial Vehicles & Trucks."@en;
    rdfs:subClassOf icb:ICB2770 .

icb:ICB2777 a owl:Class;
    rdfs:label "Servicios de transporte"@es,
        "Transportation Services"@en,
        "Transportation intesa"@nl,
        "Transportdienstleistungen"@de;
    icb:code "2777";
    rdfs:definition "Bedrijven die diensten verlenen aan de Gemotoriseerde TRANSPORTATION sector, met inbegrip van de beheerders van luchthavens, trein opslagplaatsen, wegen, bruggen en tunnels, havens en aanbieders van logistic diensten aan vervoerders van goederen. Ook dat ondernemingen vliegtuigen en wegvoertuig onderhoudsprogramma diensten."@nl,
        "Companies providing services to the Industrial Transportation sector, including companies that manage airports, train depots, roads, bridges, tunnels, ports, and providers of logistic services to shippers of goods. Includes companies that provide aircraft and vehicle maintenance services."@en,
        "Empresas proveedoras de servicios para el sector de transporteindustrial, incluidas las que administran aeropuertos, estacionesde ferrocarriles, caminos, puentes, túneles, puertos y proveedoresde servicios de logística a despachantes de productos. Incluyeempresas que proveen servicios de mantenimiento de avionesy vehículos."@es,
        "Unternehmen, die Dienstleistungen für den kommerziellen Transport anbieten. Dazu zählen Unternehmen, die Flughäfen, Zugbetriebshöfe, Straßen, Brücken, Tunnel sowie Häfen verwalten, und sowohl solche, die Logistikdienstleistungen für den Gütertransport anbieten, als auch Unternehmen, die Flugzeug- und Fahrzeugwartungsdienstleistungen erbringen."@de;
    rdfs:subClassOf icb:ICB2770 .

icb:ICB2779 a owl:Class;
    rdfs:label "LKW-Transport"@de,
        "Transporte en camión"@es,
        "Trucking"@en,
        "Trucking"@nl;
    icb:code "2779";
    rdfs:definition "Bedrijven die provide commerciele trucking diensten. Sluit wegen en tunnels exploitanten die zijn ingedeeld in TRANSPORTATION intesa en wegvoertuig verhuur- en taxi bedrijven, die zijn ingedeeld in Reis- & toerisme."@nl,
        "Companies that provide commercial trucking services. Excludes road and tunnel operators, which are classified under Transportation Services, and vehicle rental and taxi companies, which are classified under Travel & Tourism."@en,
        "Empresas que proveen servicios de transporte comercial encamión. Excluye operadores de caminos y túneles, que estánclasificados bajo Servicios de transporte, y empresas dearriendo de vehículos y taxis, que están clasificadas bajoViajes y turismo."@es,
        "Unternehmen, die LKW-Transportleistungen für den Handel anbieten. Ausgeschlossen sind Betreiber von Straßen und Tunnel (siehe: Transportdienstleistungen) sowie Mietwagenund Fuhrunternehmen (siehe: Reisen und Tourismus)."@de;
    rdfs:subClassOf icb:ICB2770 .

icb:ICB2791 a owl:Class;
    rdfs:label "Business Support Services"@en,
        "Business-Supportleistungen"@de,
        "De Ondersteunende intesa"@nl,
        "Servicios de apoyo empresarial"@es;
    icb:code "2791";
    rdfs:definition "Anbieter von nichtfinanziellen Dienstleistungen für ein breites Spektrum von Industrieunternehmen und Behörden. Dazu zählen Unternehmen im Druckgewerbe, Management-Consultants, Reinigungsunternehmen und Unternehmen, die Alarm- und Sicherheitssysteme installieren, warten und überwachen."@de,
        "De aanbieders van financiële diensten aan een breed scala van industriële ondernemingen en regeringen. Ook aanbieders van diensten, print- beheermethodes ketenprojecten, kantoorapparaten Reinigen diensten en bedrijven die te installeren, en toezicht op de onrust en zekerheid."@nl,
        "Proveedores de servicios no financieros a una amplia gamade empresas industriales y gobiernos. Incluye proveedores deservicios de impresión, consultores de gestión, serviciosde limpieza de oficinas y empresas que instalan, mantienen ymonitorizan sistemas de alarma y seguridad."@es,
        "Providers of nonfinancial services to a wide range of industrial enterprises and governments. Includes providers of printing services, management consultants, office cleaning services, and companies that install, service and monitor alarm and security systems."@en;
    rdfs:subClassOf icb:ICB2790 .

icb:ICB2793 a owl:Class;
    rdfs:label "Agencias de formaciónempresarial y de colocaciones"@es,
        "Anbieter von Business-Lehrgängen und private Arbeitsvermittler"@de,
        "Business Training & Employment Agencies"@en,
        "De Vorming & Emploi agentschappen"@nl;
    icb:code "2793";
    rdfs:definition "De aanbieders van bedrijven of managementopleiding cursussen en tewerkstellingsdiensten."@nl,
        "Proveedores de cursos de formación empresarial o administrativay servicios de colocación de personal."@es,
        "Providers of business or management training courses and employment services."@en,
        "Unternehmen, die Business- und Management-Schulungen anbieten, sowie Arbeitsvermittler."@de;
    rdfs:subClassOf icb:ICB2790 .

icb:ICB2795 a owl:Class;
    rdfs:label "Administración financiera"@es,
        "Financial Administration"@en,
        "Financiële Huishoudelijke"@nl,
        "Finanzverwaltung"@de;
    icb:code "2795";
    rdfs:definition "De aanbieders van geautomatiseerde effectenleningstransactie verwerkingen, communicatie en informatie, met inbegrip van salarismassa, zeevrachtbrief Betaling en werknemer baten- diensten."@nl,
        "Proveedores de procesamiento computarizado de transacciones,y servicios de comunicación de datos e información, incluidoslos servicios de pagos de nómina, facturas y prestaciones paraempleados."@es,
        "Providers of computerized transaction processing, data communication and information services, including payroll, bill payment and employee benefit services."@en,
        "Unternehmen, die Transaktionsverarbeitung, Datenübertragungs- und Informationsdienste anbieten, u.a. auch Gehaltsabrechnung, Zahlungssysteme und Vorsorgeleistungen für Beschäftigte."@de;
    rdfs:subClassOf icb:ICB2790 .

icb:ICB2797 a owl:Class;
    rdfs:label "Industrial Suppliers"@en,
        "Industrielieferanten"@de,
        "Industriële Toeleveranciers"@nl,
        "Proveedores industriales"@es;
    icb:code "2797";
    rdfs:definition "Distribuidores y mayoristas de productos diversificadosy equipos utilizados principalmente en los sectorescomercial e industrial. Incluye distribuidores demateriales de construcción."@es,
        "Distributors and wholesalers of diversified products and equipment primarily used in the commercial and industrial sectors. Includes builders merchants."@en,
        "Händler und Großhändler, einschließlich Bauhändler, verschiedener Produkte und Ausrüstungen, die hauptsächlich im Handel und in der Industrie genutzt werden."@de,
        "Producenten en groothandelaren van verschillende producten en apparatuur primarily gebruikt in de commerciële en industriële sector. Ook bouwlieren Merchants."@nl;
    rdfs:subClassOf icb:ICB2790 .

icb:ICB2799 a owl:Class;
    rdfs:label "Abfall- und Entsorgungsdienstleister"@de,
        "Afval & Afzet intesa"@nl,
        "Servicios de eliminaciónde desechos"@es,
        "Waste & Disposal Services"@en;
    icb:code "2799";
    rdfs:definition "Anbieter von Dienstleistungen für Schadstoffbegrenzung und Umweltschutz im Bereich Verwaltung, Rückgewinnung und Entsorgung von festen Abfällen und Gefahrstoffen, wie z.B. Mülldeponien und Recyclinghöfe. Ausgeschlossen sind Hersteller von industriellen Luft- und Wasserfiltersystemen (siehe: Industriemaschinen)."@de,
        "De aanbieders van milieuverontreiniging en de diensten voor het beheer, de nuttige toepassing en verwijdering van solide en gevaarlijke afvalstoffen, zoals landfills en recycling -stations. Sluit fabrikanten van industriële lucht en water filterverwerking equipment, die zijn ingedeeld in Gemotoriseerde Installaties."@nl,
        "Proveedores de servicios ambientales y de control de la poluciónpara el manejo, recuperación y eliminación de materiales dedesecho sólidos y peligrosos, tales como vertederos y centrosde reciclaje. Excluye fabricantes de equipo de filtración industrialde aire y agua, que están clasificados bajo Maquinaria industrial."@es,
        "Providers of pollution control and environmental services for the management, recovery and disposal of solid and hazardous waste materials, such as landfills and recycling centres. Excludes manufacturers of industrial air and water filtration equipment, which are classified under Industrial Machinery."@en;
    rdfs:subClassOf icb:ICB2790 .

icb:ICB3353 a owl:Class;
    rdfs:label "Automobile"@de,
        "Automobiles"@en,
        "Automobiles"@nl,
        "Automóviles"@es;
    icb:code "3353";
    rdfs:definition "Bereiders van motorfietsen en dieselpersonenvoertuigen, met inbegrip van auto 's, sport nutsonderneming transportmaterieel (SUV' s) en lichte vrachtwagens. Sluit bereiders van zware vrachtwagens, die zijn ingedeeld in Zakelijk Voertuigen en Trucks en bereiders van recreatievoertuigen (RVS en ATVs), die zijn ingedeeld in de zogeheten Recreational Produkten."@nl,
        "Fabricantes de motocicletas y vehículos de pasajeros, incluidosautomóviles, vehículos utilitarios deportivos (SUV) y camionesligeros. Excluye fabricantes de camiones pesados, que estánclasificados bajo Vehículos y camiones comerciales, y fabricantesde vehículos recreativos como caravanas y todoterrenos (RV yATV), que están clasificados bajo Productos recreativos."@es,
        "Hersteller von Motorrädern, Personenkraftwagen, inklusive Automobile, Geländelimousinen (SUV) und leichte Nutzfahrzeuge. Ausgeschlossen sind Hersteller von Schwerfahrzeugen, die als Lastkraftwagen klassifiziert werden, sowie Hersteller von Freizeitfahrzeugen (Wohnmobile und Geländewagen), die unter Freizeitprodukte zu klassifizieren sind."@de,
        "Makers of motorcycles and passenger vehicles, including cars, sport utility vehicles (SUVs) and light trucks. Excludes makers of heavy trucks, which are classified under Commercial Vehicles &amp; Trucks, and makers of recreational vehicles (RVs and ATVs), which are classified under Recreational Products."@en;
    rdfs:subClassOf icb:ICB3350 .

icb:ICB3355 a owl:Class;
    rdfs:label "Auto Delen"@nl,
        "Auto Parts"@en,
        "Autoteile"@de,
        "Repuestos de automóviles"@es;
    icb:code "3355";
    rdfs:definition "Fabricantes y distribuidores de piezas nuevas y repuestos paramotocicletas y automóviles, tales como motores, carburadoresy baterías. Excluye productores de neumáticos, que estánclasificadas bajo Neumáticos."@es,
        "Fabrikanten en distributeurs van nieuwe en reserveonderdelen voor motorfietsen en auto 's, zoals Motoren, carburettors en batterijen. Dat producenten van banden, die zijn ingedeeld."@nl,
        "Manufacturers and distributors of new and replacement parts for motorcycles and automobiles, such as engines, carburettors and batteries. Excludes producers of tires, which are classified under Tires."@en,
        "Vertriebsgesellschaften und Hersteller von Neu- und Ersatzteilen für Motorräder und Automobile wie Motoren, Vergaser und Batterien. Ausgeschlossen sind Reifenhersteller. Diese werden unter Reifen klassifiziert."@de;
    rdfs:subClassOf icb:ICB3350 .

icb:ICB3357 a owl:Class;
    rdfs:label "Banden"@nl,
        "Neumáticos"@es,
        "Reifen"@de,
        "Tires"@en;
    icb:code "3357";
    rdfs:definition "De producenten, distributeurs en retreaders van auto 's, vrachtwagens en motorrijders."@nl,
        "Fabricantes, distribuidores y recauchadoras de neumáticos paraautomóviles, camiones y motocicletas."@es,
        "Manufacturers, distributors and retreaders of automobile, truck and motorcycle tires."@en,
        "Vertriebsgesellschaften, Zwischenhändler und Hersteller von Motorrad- und Automobilreifen."@de;
    rdfs:subClassOf icb:ICB3350 .

icb:ICB3533 a owl:Class;
    rdfs:label "Brauereien"@de,
        "Brewers"@en,
        "Brouwers"@nl,
        "Cerveceras"@es;
    icb:code "3533";
