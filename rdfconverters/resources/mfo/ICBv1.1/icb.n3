        "Oil & Gas"@en,
        "Petróleo y gas"@es;
    rdfs:comment ""^^xsd:string;
    rdfs:subClassOf icb:ICB .

icb:ICB1300 a owl:Class;
    icb:code "1300";
    rdfs:label "Chemicals"@en,
        "Chemie"@de,
        "Productos químicos"@es;
    rdfs:subClassOf icb:ICB1000 .

icb:ICB2300 a owl:Class;
    icb:code "2300";
    rdfs:label "Bauwesen und Materialien"@de,
        "Construcción y materiales"@es,
        "Construction & Materials"@en;
    rdfs:subClassOf icb:ICB2000 .

icb:ICB3300 a owl:Class;
    icb:code "3300";
    rdfs:label "Automobiles & Parts"@en,
        "Automobilhersteller und Zulieferer"@de,
        "Automóviles y repuestos"@es;
    rdfs:subClassOf icb:ICB3000 .

icb:ICB3780 a owl:Class;
    icb:code "3780";
    rdfs:label "Tabaco"@es,
        "Tabakwaren"@de,
        "Tobacco"@en;
    rdfs:subClassOf icb:ICB3700 .

icb:ICB4000 a owl:Class;
